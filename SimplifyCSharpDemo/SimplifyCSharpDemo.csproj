  <ItemGroup>
    <ProjectReference Include="..\SimplifyCSharp\SimplifyCSharp.csproj" />
  </ItemGroup>

  <!-- WPF files (App.xaml, MainWindow.xaml) are included implicitly by the SDK. -->

</Project>