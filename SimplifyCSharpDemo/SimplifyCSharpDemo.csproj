<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net10.0-windows;net6.0-windows</TargetFrameworks>
    <UseWPF>true</UseWPF>
    <AssemblyName>SimplifyCSharpDemo</AssemblyName>
    <OutputType>WinExe</OutputType>
